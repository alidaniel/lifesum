<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <gradient
                android:centerX="0.5"
                android:centerY="0.3"
                android:endColor="#aebfb4"
                android:gradientRadius="300dp"
                android:startColor="#f8fafa"
                android:type="radial" />
        </shape>
    </item>

    <item
        android:width="@dimen/width"
        android:height="@dimen/height"
        android:top="@dimen/peas_top"
        android:left="@dimen/peas_left"
        android:drawable="@drawable/peas">
    </item>

    <item
        android:width="@dimen/width"
        android:height="@dimen/height"
        android:top="@dimen/orange_top"
        android:left="@dimen/orange_left"
        android:drawable="@drawable/orange">
    </item>

    <item
        android:width="@dimen/salad_width"
        android:height="@dimen/salad_height"
        android:top="@dimen/salad_top"
        android:left="@dimen/salad_left"
        android:bottom="@dimen/salad_bottom"
        android:drawable="@drawable/salad"/>

</layer-list>

<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <gradient
                android:centerX="0.5"
                android:centerY="0.3"
                android:endColor="#cec7c0"
                android:gradientRadius="300dp"
                android:startColor="#f1eeeb"
                android:type="radial" />
        </shape>
    </item>

    <item
        android:top="100dp"
        android:left="-30dp"
        android:width="88dp"
        android:height="88dp"
        android:drawable="@drawable/orange"/>

    <item
        android:top="400dp"
        android:left="350dp"
        android:width="68dp"
        android:height="68dp"
        android:drawable="@drawable/orange"/>

    <item
        android:top="480dp"
        android:left="-20dp"
        android:width="128dp"
        android:height="128dp"
        android:drawable="@drawable/orange"/>


</layer-list>
